<section class="events_background">
	<div id="wrapper">
<div class="container">
	<h1 class="text-center browse-header">Profiles</h1>
			<% @profiles.each do |profile| %>
				<p class="inline-block"><%= image_tag profile.image.url(:medium) %></br>
				<%= link_to(profile.name, profile_path(profile.id)) %></p>
			<% end %> 
</div>
    <%= will_paginate @profiles %>

</div>


</section>

	
