<div class= "wrapper text-center">
  <h1>Artist <strong>"Dating"</strong></h1>
  <h2>Are you looking for love in all the wrong places?</h2>
  <p>
  <%= link_to "Musicians", musicians_path, class: "btn btn-warning btn-lrg" %>
  <%= link_to "Artists", visual_artists_path, class: "btn btn-warning btn-lrg" %>
  <%= link_to "Venue Reps", venue_reps_path, class: "btn btn-warning btn-lrg" %>
  </p>
  <div class="container">
    <h1 class="text-center browse-header">Profiles</h1>
        <% @profiles.each do |profile| %>
          <p class="inline-block"><%= image_tag profile.image.url(:medium) %></br>
          <%= link_to(profile.name, profile_path(profile.id), class: "btn btn-warning btn-md") %></p>
        <% end %> 
  </div>

</div>
    <a class="text-center"><%= will_paginate @profiles %></a>

</div>


</section>

