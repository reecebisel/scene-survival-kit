<h1> NEW PROFILE </h1>
<%= form_for @profile, html: {multipart: true} do |f| %>
  <%= f.label(:image)%>
	<%= f.file_field(:image)%>
	<br/>
	<%= f.label(:name)%>
	<%= f.text_field(:name)%>
	<br/>
	<%= f.label(:phone_number)%>
	<%= f.text_field(:phone_number)%>
	<br/>
	<%= f.label(:bio)%> 
	<%= f.text_area(:bio)%>
	<br/>
	<%= f.submit("Submit")%>
<%end%>

